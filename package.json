{
  "name": "@agecheck/node",
  "version": "0.2.0",
  "description": "AgeCheck Node SDK for gate policy, JWT verification, and signed verification cookies.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/agecheck/agecheck-node"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agecheck/core": "^0.2.0"
  },
  "devDependencies": {
    "@agecheck/core": "^0.2.0",
    "@types/node": "^24.5.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@10.29.2"
}
